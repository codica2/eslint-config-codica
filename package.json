{
  "name": "eslint-config-codica",
  "version": "1.0.0",
  "description": "This package provides Codica's .eslintrc as an extensible shared config.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint index.js",
    "lint:write": "./node_modules/.bin/eslint --debug . --fix",
    "prettier": "./node_modules/.bin/prettier --write ./**/*.js"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "codica",
    "react"
  ],
  "author": "Alex Malinin (https://github.com/alexmalinin)",
  "license": "MIT",
  "lint-staged": {
    "*.(js|jsx)": [
      "yarn lint:write",
      "git add"
    ]
  },
  "dependencies": {
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0"
  },
  "peerDependencies": {
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0"
  }
}
