{
  "name": "@codica/eslint-config-react",
  "version": "1.0.0",
  "description": "This package provides Codica's .eslintrc as an extensible shared config.",
  "author": "Eugene Volkov (https://github.com/jk-volkov)",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/codica2/eslint-config-codica"
  },
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "compile": "tsc",
    "lint": "eslint index.js",
    "lint:fix": "./node_modules/.bin/eslint . --fix",
    "prettier": "./node_modules/.bin/prettier --write ./**/*.js",
    "publish": "npm publish --access public"
  },
  "eslintConfig": {
    "extends": "./index.js"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "airbnb",
    "react.js",
    "codica"
  ],
  "dependencies": {
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unused-imports": "^1.1.1"
  },
  "peerDependencies": {
    "eslint": ">=7.0",
    "prettier": ">=2.0",
    "react": ">=16.0"
  }
}
